<%= render :partial => "nav" %>

<h2>Add new worklog</h2>


<%= labelled_form_for @worklog, :url => worklogs_path, :html => {:id => 'worklog-form'} do |f| %>
    <%= render :partial => 'form', :locals => {:f => f} %>
    <%= submit_tag 'Create' %>
    <!--%= preview_link preview_worklogs_path(), 'worklog-form' %-->
<% end %>

<div id="preview" class="worklog"></div>

