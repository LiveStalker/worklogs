<% @issues.each do |issue| %>
    <% if issue.custom_field_value(@estimate_dev).nil? %>
        <%= issue.status %>
        <%= issue.assigned_to %>
        <%= worklogs_textilizable('#' + issue.id.to_s) %>
    <% end %>
<% end %>
<%= textilizable(Setting.plugin_worklogs['WORKLOGS_MAIL_FOOTER']) %>
